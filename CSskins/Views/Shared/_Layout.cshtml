@RenderBody()
<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>HotCase</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.4/font/bootstrap-icons.css">
    <link rel="stylesheet" href="css/style.css">
    <link rel="stylesheet" href="css/site.css">
</head>
<body>
    <header id="cabecalho" class="sticky-top">
        <div class="navbar navbar-expand-lg">
            <div class="container">
                <h1 class="text-branco"><span class="text-vermelho">Hot</span>Case</h1>
                <button class="navbar-toggler back_branco" data-bs-toggle="collapse" data-bs-target="#menu">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <nav class="collapse navbar-collapse justify-content-end" id="menu">
                    <ul class="nav navbar-nav nav-pills">
                        <li class="nav-item">
                            <a class="nav-link active" href="#inicio">NewCase</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#jogos">VIP Case</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#novidades">Regular Case</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#contato">Contact</a>
                        </li>
                    </ul>
                </nav>
                    <partial name="_LoginPartial"> 
            </div>
        </div>
    </header>
    <div data-bs-spy="scroll" data-bs-smooth-scroll="true" data-bs-target="#cabecalho">
        <section id="inicio" class="pt-5 pb-5">
            <div class="container">
                <h3 class="fw-bold text-center fs-1 mb-5">Uncover hot scenarios with <span class="text-vermelho">Hot</span>Case</h3>
                <div class="carousel slide" id="carousel-image" data-bs-ride="carousel">
                    <div class="carousel-inner">
                        <div data-bs-interval="2000" class="carousel-item active">
                            <img src="https://tecmasters.com.br/wp-content/uploads/2023/03/Captura-de-CSGO_Easy-Resize.com_.jpg" alt="">
                        </div>
                        <div data-bs-interval="2000" class="carousel-item">
                            <img src="https://tecmasters.com.br/wp-content/uploads/2023/03/Captura-de-CSGO_Easy-Resize.com_.jpg" alt="">
                        </div>
                        <div data-bs-interval="2000" class="carousel-item">
                            <img src="https://tecmasters.com.br/wp-content/uploads/2023/03/Captura-de-CSGO_Easy-Resize.com_.jpg" alt="">
                        </div>
                    </div>
                    <button type="button" class="carousel-control-prev" data-bs-slide="prev" data-bs-target="#carousel-image">
                        <span class="carousel-control-prev-icon"></span>
                    </button>
                    <button type="button" class="carousel-control-next" data-bs-slide="next" data-bs-target="#carousel-image">
                        <span class="carousel-control-next-icon"></span>
                    </button>
                </div>

            </div>
        </section>
        <section id="jogos">
            <div class="container">
                <div class="row">
                    <div class="col-md-8">
                        <div class="card border-4 p-4 pb-5">
                            <header class="card-header text-center border-4 m-4 p-4 pb-4">
                                <h1 class="fs-3 w-4"><span class="text-vermelho">VIP</span>Case</h1>
                            </header>
                            <div class="card-body text-center p-0">
                                <img class="w-65" src="https://images.steamcdn.io/csgonet/cases/brilliants.png" alt="">
                                
                                <ul>
                                </ul>
                                <li class="fw-bold mb-0 pt-2">
                                    VIP case is an exclusive package with rare items available to a limited audience of players. It contains unique and valuable game items that can be highly valued in the CS:GO gaming market.
                                </li>
                                </ul>
                            </div>
                        </div>
                        <div class="card border-5 p-5 pb-5">
                            <header class="card-header text-center border-4 m-4 p-4 pb-4">
                                <h1 class="fs-3 w-100">VIP <span class="text-vermelho">KnifeCase</span></h1>
                            </header>

                            <div class="card-body text-center p-0">
                                <img class="w-65 mx-auto" src="https://images.steamcdn.io/csgonet/cases/fade-mining.png" alt="">
                                <li class="fw-bold mb-0 pt-2">
                                    The Knife Case in Counter-Strike: Global Offensive is an amazing opportunity to get exclusive knives
                                </li>
                            </div>
                        </div>

                        <div class="card border-4 p-4 pb-5">
                            <header class="card-header text-center border-4 m-4 p-4 pb-4">
                                <h6 class="fs-3 w-65">VIP <span class="text-vermelho">Gloves Case</span></h6>
                            </header>
                            <div class="card-body text-center p-3">
                                <img class="w-65" src="https://images.steamcdn.io/csgonet/cases/luxury-gloves-mining.png" alt="">
                                <li class="fw-bold mb-0 pt-2">
                                    The Counter-Strike: Global Offensive Glove Case is your way to stylish and unique gloves for your game character.
                                </li>
                                <ul>
                                </ul>

                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4 ps-3">
                        <h3>Popular Case</h3>
                        <div class="texto-traco mb-3"></div>
                        <div class="row">
                            <div class="col-4 pb-3">
                                <a href="#">
                                    <img class="w-100 h-100" src="https://images.steamcdn.io/csgonet/cases/barbie.png" alt="">
                                </a>
                            </div>
                            <div class="col-8 p-0">
                                <a href="#" class="text-decoration-none text-cor-site">
                                    <p class="fw-bold">
                                        Rayan Gosling Case
                                    </p>
                                </a>
                            </div>
                            <div class="col-4 pb-3">
                                <a href="#">
                                    <img class="w-100 h-100" src="https://images.steamcdn.io/csgonet/cases/smurf-cat.png" alt="">
                                </a>
                            </div>
                            <div class="col-8 p-0">
                                <a href="#" class="text-decoration-none text-cor-site">
                                    <p class="fw-bold">
                                        Smurf cat Case
                                    </p>
                                </a>
                            </div>
                            <div class="col-4 pb-3">
                                <a href="#">
                                    <img class="w-100 h-100" src="https://images.steamcdn.io/csgonet/cases/bloody-mate.png" alt="">
                                </a>
                            </div>
                            <div class="col-8 p-0">
                                <a href="#" class="text-decoration-none text-cor-site">
                                    <p class="fw-bold">
                                        Bloody Buddy Case
                                    </p>
                                </a>
                            </div>
                            <div class="col-4 pb-3">
                                <a href="#">
                                    <img class="w-100 h-100" src="https://images.steamcdn.io/csgonet/cases/gym.png" alt="">
                                </a>
                            </div>
                            <div class="col-8 p-0">
                                <a href="#" class="text-decoration-none text-cor-site">
                                    <p class="fw-bold">
                                        Sigma Case
                                    </p>
                                </a>
                            </div>
                            <div class="col-4 pb-3">
                                <a href="#">
                                    <img class="w-100 h-100" src="https://images.steamcdn.io/csgonet/cases/aquarium.png" alt="">
                                </a>
                            </div>
                            <div class="col-8 p-0">
                                <a href="#" class="text-decoration-none text-cor-site">
                                    <p class="fw-bold">
                                        Aquarium Case
                                    </p>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <section id="novidades" class="pb-5">
            <div class="container">
                <h3>
                    Regual <span class="text-vermelho">Case</span>
                </h3>
                <div class="texto-traco-2 mb-5"></div>
                <div class="row">
                    <div class="col-md-4">
                        <div class="card border-0">
                            <header class="card-header border-0">
                                <img class="w-100 rounded" src="https://images.steamcdn.io/csgonet/cases/free-silver2.png" alt="">
                            </header>
                            <div class="card-body border-0">
                                <h5>
                                    <a class="novidades-links" href="#">
                                        Silver <span class="text-vermelho">Case</span>
                                </h5>
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="card border-0">
                            <header class="card-header border-0">
                                <img class="w-100 rounded" src="https://images.steamcdn.io/csgonet/cases/free-nova.png" alt="">
                            </header>
                            <div class="card-body border-0">
                                <a class="novidades-links" href="#">
                                    <h5>Nova <span class="text-vermelho">Case</span></h5>
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="card border-0">
                            <header class="card-header border-0">
                                <img class="w-100 rounded" src="https://images.steamcdn.io/csgonet/cases/free-mg.png" alt="">
                            </header>
                            <div class="card-body border-0">
                                <h5><a class="novidades-links" href="#">Master Guardian <span class="text-vermelho">Case</span></a></h5>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="card border-0">
                            <header class="card-header border-0">
                                <img class="w-100 rounded" src="https://images.steamcdn.io/csgonet/cases/free-lem.png" alt="">
                            </header>
                            <div class="card-body border-0">
                                <h5>
                                    <a class="novidades-links" href="#">
                                        Legendary Eagle <span class="text-vermelho">Case</span>
                                </h5>

                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="card border-0">
                            <header class="card-header border-0">
                                <img class="w-100 rounded" src="https://images.steamcdn.io/csgonet/cases/free-supreme2.png" alt="">
                            </header>
                            <div class="card-body border-0">
                                <a class="novidades-links" href="#">
                                    <h5>Supreme <span class="text-vermelho">Case</span></h5>
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="card border-0">
                            <header class="card-header border-0">
                                <img class="w-100 rounded" src="https://images.steamcdn.io/csgonet/cases/free-global.png" alt="">
                            </header>
                            <div class="card-body border-0">
                                <h5><a class="novidades-links" href="#">Global Elite <span class="text-vermelho">Case</span></a></h5>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <section id="contato" class="pb-5 mb-5">
            <div class="container">
                <h3>Сontact us</h3>
                <div class="texto-traco-2"></div>
                <div class="row">
                    <div class="col-md-7">
                        <form class="mt-5">
                            <label for="nome" class="form-label">Name</label>
                            <input type="text" class="form-control" name="nome" id="nome" required>
                            <label for="email" class="form-label">E-mail</label>
                            <input type="email" class="form-control" name="email" id="email" required>
                            <label for="telefone" class="form-label">Phone (optional)</label>
                            <input type="tel" class="form-control" name="telefone" id="telefone">
                            <label for="mensagem" class="form-label">Your message</label>
                            <textarea name="mensagem" id="mensagem" class="form-control" required></textarea>
                            <button type="submit" class="btn botao_site">Іend</button>
                        </form>
                    </div>
                    <div class="col-md-5">
                        <h3 class="mt-5">Social NetWork</h3>
                        <div class="redes-sociais-links">
                            <ul class="p-0">
                                <li>
                                    <a href="#">
                                        <i class="bi bi-facebook fs-3 text-cor-site"></i>
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <i class="bi bi-instagram fs-3 text-cor-site ms-2 me-2"></i>
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <i class="bi bi-whatsapp fs-3 text-cor-site"></i>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
    <footer class="pb-1 pt-4">
        <p class="text-center">
            <span class="text-vermelho">&copy; Copyright 2023</span> - HotCase - All rights reserved.
        </p>
    </footer>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"></script>
    <script src="js/jquery-3.6.4.min.js"></script>
    <script src="js/jquery.mask.min.js"></script>
    <script src="js/main.js"></script>
    @await RenderSectionAsync("Scripts", required: false);
</body>
</html>